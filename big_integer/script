pva701@notebook:~/QtProjects/big_integer/gtest/make$ make
g++ -isystem ../include -g -Wall -Wextra -pthread -c ../samples/big_integer.cpp
g++ -isystem ../include -g -Wall -Wextra -pthread -c ../samples/gtest_unittest.cpp
g++ -isystem ../include -I.. -g -Wall -Wextra -pthread -c \
            ../src/gtest-all.cc
g++ -isystem ../include -I.. -g -Wall -Wextra -pthread -c \
            ../src/gtest_main.cc

ar rv gtest_main.a gtest-all.o gtest_main.o

g++ -isystem ../include -g -Wall -Wextra -pthread -lpthread big_integer.o gtest_unittest.o gtest_main.a -o gtest_unittest
